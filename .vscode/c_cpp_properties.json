{
    "version": 4,
    "configurations": [
        {
            "name": "Firmware",
            "includePath": [
                "${workspaceFolder}/app/**",
                "${workspaceFolder}/targets/**",
                "${workspaceFolder}/libs/**",
                "${workspaceFolder}/platforms/hw/**"
            ],
            "defines": [
                "ARM_MATH_CM4",
                "STM32FXX",
                "USE_STDPERIPH_DRIVER"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/app/**",
                    "${workspaceFolder}/targets/**",
                    "${workspaceFolder}/libs/**",
                    "${workspaceFolder}/platforms/hw/**"
                ]
            },
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "configurationProvider": "go2sh.cmake-integration"
        },
        {
            "name": "Desktop",
            "includePath": [
                "${workspaceFolder}/app/**",
                "${workspaceFolder}/targets/desktop/**",
                "${workspaceFolder}/platforms/emu/**"
            ],
            "defines": [
                "ARM_MATH_CM4",
                "STM32FXX",
                "USE_STDPERIPH_DRIVER"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/app/**",
                    "${workspaceFolder}/platforms/emu/**",
                    "${workspaceFolder}/targets/desktop/**"
                ]
            },
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "configurationProvider": "go2sh.cmake-integration"
        }
    ]
}